{
  "name": "@kronos-integration/service-health-check",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/service-health-check.mjs",
  "exports": {
    ".": "./src/service-health-check.mjs"
  },
  "description": "collects and reports system health status",
  "keywords": [
    "kronos-service"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/service-health-check.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/service-health-check.mjs"
  },
  "dependencies": {
    "@kronos-integration/endpoint": "^9.2.12",
    "@kronos-integration/service": "^9.2.18",
    "model-attributes": "^4.1.7"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.4",
    "documentation": "^13.0.2",
    "semantic-release": "^17.2.1"
  },
  "engines": {
    "node": ">=14.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-health-check.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-health-check/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-health-check#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-github-action",
      "arlac77/template-kronos-component"
    ]
  }
}
